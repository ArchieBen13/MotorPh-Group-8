            overtimePay = 0;
            netPay = regPay;
        } else {
            weeklySalary = 200;
            overTime = weeklyTime - 40;
            regPay = basicSalary * weeklyTime;
            overtimePay = basicSalary * overTime;
            netPay = regPay + overtimePay;
        }

